--game elements

player = entity_class:new{
	width=6,
	sp=4, arms_sp=80, 	--sprites
	dir=1, 	--dir: -1=left 1=right
	vx=0, vy=0, 	--velocity
	--states
	is_dead=false,
	is_floored=false,
	is_climbing=false,
	update = function(_ENV, scene)
		if (is_dead) then
			sp = 6
			goto continue
		end
	
		dx,dy=0,0
		if (btn(‚¨ÖÔ∏è)) dx -= 1
		if (btn(‚û°Ô∏è)) dx += 1
		is_moving = dx != 0
		if (is_moving) dir = dx
		
		if (is_floored and btn(üÖæÔ∏è)) vy -= 5 	--jump force
		
		vx += dx*0.6 	--walk force
		vy += 0.7 	--gravity
		vy = mid(-height, vy, height)
		
		--apply velocity
		x += vx
		y += vy
		
		--map collisions
		is_floored=false
		is_climbing=false
		if (vx!=0 or vy!=0) then
			--vertical collisions
			entity_class.update(_ENV)
			if (vy>0 and (is_solid(x-vx+0.5,ey) or is_solid(ex-vx-0.5,ey))) then
				y = flr(ey/8)*8 - height+0.5
				vy = 0
				is_floored=true
			elseif (vy>0 and (is_ladder(x-vx+0.5,ey) or is_ladder(ex-vx-0.5,ey))) then
				if (btn(‚¨áÔ∏è)) then
					vy = min(vy, 1) 	--descend speed
				else
					if (is_ladder(x-vx+0.5,y) or is_ladder(ex-vx-0.5,y)) then
						y -= vy
					else
						y = flr(ey/8)*8 - height+0.5
					end
					vy = 0
					is_floored=true
				end
				is_climbing=true
			elseif (vy<0 and (is_solid(x-vx+0.5,y) or is_solid(ex-vx-0.5,y))) then
				y = flr(ey/8)*8
				vy = 0
			end
			--horizontal collisions
			entity_class.update(_ENV)
			if (vx>0 and (is_solid(ex,y) or is_solid(ex,ey-0.5))) then
				x = flr(ex/8)*8 - width+0.5
			elseif (vx<0 and (is_solid(x,y) or is_solid(x,ey-0.5))) then
				x = flr(ex/8)*8
			end
		end
		
		--entity collisions
		for e in all(scene.entities) do
			if (e:collide(_ENV)) then
				--todo
			end
		end
		
		--apply friction
		vx *= 0.6
		vy *= 0.98
		
		--sprite update
		if (is_floored) then
			sp = is_moving and sp+0.4 or 1
			if (sp >= 6) sp-=5
		else
			sp = vy<0 and 3 or 4
		end
		arm_sp = max(sp + 78, 80)
		
		::continue::
		entity_class.update(_ENV)
	end,
	draw = function(_ENV)
		spr(sp, x-0.75+0.75*sgn(dir), y, 1, 1, dir<0)
		if (not is_dead) spr(arm_sp, x-0.75+0.75*sgn(dir), y, 1, 1, dir<0)
	end,
}

--map utils
function is_solid(x,y)
	if (x<0) return true
	x = flr(x/8)
	y = flr(y/8)
	return fget(mget(x,y),0)
end

function is_ladder(x,y)
	x = flr(x/8)
	y = flr(y/8)
	return fget(mget(x,y),1)
end

function is_dangerous(x,y)
	x = flr(x/8)
	y = flr(y/8)
	return fget(mget(x,y),2)
end

tilemap_class = entity_class:new{
	new = function(self, body)
		body = body or {}
		body.tiles = body.tiles or {}
		return entity_class.new(self, body)
	end,
	load_map = function(_ENV, sx, sy)
		sx = sx or 0
		sy = sy or 0
		for i=0,width-1 do
			for j=0,height-1 do
				_ENV:set(i, j, mget(sx+i, sy+j))
			end
		end
	end,
	apply_map = function(_ENV, sx, sy)
		sx = sx or 0
		sy = sy or 0
		for i=0,width-1 do
			for j=0,height-1 do
				mset(sx+i, sy+j, _ENV:get(i, j))
			end
		end
	end,
	load_str = function(_ENV, str)
		for i=1,#str do
			tiles[i] = ord(sub(str,i,i))-16
		end
	end,
	save_str = function(_ENV)
		local str = ""
		for j=0,height-1 do
			for i=0,width-1 do
				local c = chr(_ENV:get(i,j)+16)
				str ..= c
			end
		end
		return str
	end,
	get = function(_ENV, x, y)
		return tiles[1 + y*width + x] or 0
	end,
	set = function(_ENV, x, y, v)
		tiles[1 + y*width + x] = v
	end,
	update = function(_ENV)
		ex = x + width*8 -1
		ey = y + height*8 -1
	end,
	draw = function(_ENV)
		for i=0,width-1 do
			for j=0,height-1 do
				local tile=_ENV:get(i, j)
				if (tile > 0) spr(tile, x + i*8, y + j*8)
			end
		end
	end,
}

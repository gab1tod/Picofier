global = _ENV
class = setmetatable({
	new = function(self, body)
		body = body or {}
		return setmetatable(body, { __index = self })
	end,
	is = function(self, t)
		if (self == t) return true
		local mt = getmetatable(self)
		if (mt.__index == nil or mt.__index.is == nil) return nil
		return mt.__index:is(t)
	end
},{ __index = _ENV })


--sort table
function sort(t,comp)
	comp=comp or function(a,b) return a<b end
	for i=1,#t do
		local j=i
		while j>1 and comp(t[j],t[j-1]) do
			t[j],t[j-1]=t[j-1],t[j]
			j-=1
		end
	end
end

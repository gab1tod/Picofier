--magamen

entity_class = class:new{
	x=0, y=0, z=0, 	--position
	ex=0, ey=0,
	width=8, height=8, 	--size
	update = function(_ENV)
		ex = x + width-0.5
		ey = y + height-0.5
	end,
	collide = function(_ENV, other)
		return ex >= other.x and x <= other.ex and ey >= other.y and y <= other.ey
	end,
	event = function(self,t,scene,data) 	--event type, scene and data
		if (self[t]) self[t](self,scene,data)
	end
}

camera_class = entity_class:new{
	width=128, height=128
}

scene_class = class:new{
	new = function(self, body)
		body = body or {}
		body.cam = body.cam or camera_class:new{}
		return class.new(self, body)
	end,
	cam = camera_class:new{},
	entities = {},
	update = function(_ENV)
		_ENV:event("update")
	end,
	draw = function(_ENV)
		sort(entities, function(a, b) return a.z<b.z end)
		_ENV:event("draw")
	end,
	event = function(_ENV,t,data)
		for e in all(entities) do
			e:event(t,_ENV,data)
		end
	end
}
